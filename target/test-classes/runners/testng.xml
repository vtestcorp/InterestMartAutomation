<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Interest Smart Test Automtion Regression Suite">

	<listeners>
		<listener class-name="com.qa.interestsmart.listeners.ExtentReportListener" />
		<listener class-name="com.qa.interestsmart.listeners.ScreenshotListener" />
	</listeners>

	<test name="Sign In With Username Password Test On Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
		    <class name="com.qa.interestsmart.tests.TC_11_Search_Address_Without_Login_Then_Login"/>
		 	<class name="com.qa.interestsmart.tests.TC_02_LoginFacebook_Verify_FirstName_LastName_Logout"/>
			<class name="com.qa.interestsmart.tests.TC_03_Login_ValidEmail_ValidPassword"/>
			<class name="com.qa.interestsmart.tests.TC_04_Login_InvalidEmail_ValidPassword"/>
			<class name="com.qa.interestsmart.tests.TC_05_Login_ValidEmail_InvalidPassword"/>
			<class name="com.qa.interestsmart.tests.TC_06_InvalidEmail_InvalidPassword"/>
			<class name="com.qa.interestsmart.tests.TC_07_SearchLoan_ByValidAddress"/>
			<class name="com.qa.interestsmart.tests.TC_08_SearchLoan_ByInValidAddress"/>
			<class name="com.qa.interestsmart.tests.TC_09_Verify_MonthlyPayment_RemainingBalance"/>	
			<class name="com.qa.interestsmart.tests.TC_10_Verify_MonthlyPayment_RemainingBalance_NegativeScenario"/>		
			<class name="com.qa.interestsmart.tests.TC_12_Submit_Loan"/>								
		</classes>
	</test>
</suite>